
@article{outeiro_yeast_2003,
	title = {Yeast {Cells} {Provide} {Insight} into {Alpha}-{Synuclein} {Biology} and                     {Pathobiology}},
	volume = {302},
	issn = {0036-8075},
	url = {https://www.ncbi.nlm.nih.gov/pmc/articles/PMC1780172/},
	doi = {10.1126/science.1090439},
	abstract = {Alpha-synuclein is implicated in several neurodegenerative disorders,
                    such as Parkinson’s disease and multiple system atrophy, yet its
                    functions remain obscure. When expressed in yeast, alpha-synuclein associated
                    with the plasma membrane in a highly selective manner, before forming
                    cytoplasmic inclusions through a concentration-dependent, nucleated process.
                    Alpha-synuclein inhibited phospholipase D, induced lipid droplet accumulation,
                    and affected vesicle trafficking. This readily manipulable system provides an
                    opportunity to dissect the molecular pathways underlying normal alpha-synuclein
                    biology and the pathogenic consequences of its misfolding.},
