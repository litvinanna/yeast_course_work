	urldate = {2018-05-02},
	journal = {European Journal of Biochemistry},
	author = {Shigeo, Nakajo and Kazuhiko, Tsukada and Kumiko, Omata and Yasuharu, Nakamura and Kazuyasu, Nakaya},
