@article{oien_clearance_2009,
	title = {Clearance and {Phosphorylation} of {Alpha}-{Synuclein} {Are} {Inhibited} in {Methionine} {Sulfoxide} {Reductase} {A} {Null} {Yeast} {Cells}},
	volume = {39},
	issn = {0895-8696, 1559-1166},
	url = {http://link.springer.com/10.1007/s12031-009-9274-8},
	doi = {10.1007/s12031-009-9274-8},
	abstract = {Aggregated α-synuclein and the point mutations Ala30Pro and Ala53Thr of α-synuclein are associated with Parkinson’s disease. The physiological roles of α-synuclein and methionine oxidation of the α-synuclein protein structure and function are not fully understood. Methionine sulfoxide reductase A (MsrA) reduces methionine sulfoxide residues and functions as an antioxidant. To monitor the effect of methionine oxidation to α-synuclein on basic cellular processes, α-synucleins were expressed in msrA null mutant and wild-type yeast cells. Protein degradation was inhibited in the α-synuclein-expressing msrA null mutant cells compared to α-synuclein-expressing wild-type cells. Increased inhibition of degradation and elevated accumulations of fibrillated proteins were observed in SynA30P-expressing msrA null mutant cells. Additionally, methionine oxidation inhibited α-synuclein phosphorylation in yeast cells and in vitro by casein kinase 2. Thus, a compromised MsrA function combined with α-synuclein overexpression may promote processes leading to synucleinopathies.},
	language = {en},
	number = {3},
	urldate = {2018-05-20},
	journal = {Journal of Molecular Neuroscience},
	author = {Oien, Derek B. and Shinogle, Heather E. and Moore, David S. and Moskovitz, Jackob},
