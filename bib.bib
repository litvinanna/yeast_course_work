@article{murray_synucleinopathies:_2001,
	title = {Synucleinopathies: a pathological and molecular review},
	volume = {1},
	issn = {15662772},
	shorttitle = {Synucleinopathies},
	url = {http://linkinghub.elsevier.com/retrieve/pii/S1566277201000238},
	doi = {10.1016/S1566-2772(01)00023-8},
	abstract = {Synucleins are a family of small, presynaptic neuronal proteins comprised of a-, b-, g-synucleins and synoretin, of which only a-synuclein aggregates have been associated with several neurological diseases. The normal neuronal function of synucleins are presently unknown, but several activities such as lipid vesicle binding, inhibition of phospholipase D2 and protein kinase C, dopamine uptake and as a chaperone have been ascribed to a-synuclein. The role of synuclein in the etiology of neuropathology has developed from several observations. Pathologically, synuclein was identiﬁed as the major component of Lewy bodies (LBs), the hallmark inclusions of Parkinson’s disease (PD), and a fragment thereof was isolated from amyloid plaques of a different neurological disease, Alzheimer’s disease (AD). Biochemically, recombinant a-synuclein was shown to be able to form ﬁbrils which recapitulated the ultrastructural features of a-synuclein isolated from patients with dementia with LBs (DLB), PD and multiple system atrophy (MSA). Additionally, the identiﬁcation of mutations within the synuclein gene, albeit in rare cases of familial PD, demonstrated an unequivocal link between synuclein pathology and neurodegenerative disease. The common involvement of a-synuclein in a spectrum of diseases such as PD, DLB, MSA and the LB variant of AD has led to the classiﬁcation of these diseases under the umbrella term of synucleinopathies. q 2001 Elsevier Science B.V. All rights reserved.},
	language = {en},
	number = {6},
	urldate = {2018-04-05},
	journal = {Clinical Neuroscience Research},
	author = {Murray, Ian V.J. and Lee, Virginia M.-Y. and Trojanowski, John Q.},
	month = dec,
	year = {2001},
	pages = {445--455},
	file = {Murray и др. - 2001 - Synucleinopathies a pathological and molecular re.pdf:/Users/pochtalionizm/.zotero/storage/R4AYX56S/Murray и др. - 2001 - Synucleinopathies a pathological and molecular re.pdf:application/pdf}
}

@article{franssens_benefits_2013,
	title = {The {Benefits} of {Humanized} {Yeast} {Models} to {Study} {Parkinson}'s {Disease}},
