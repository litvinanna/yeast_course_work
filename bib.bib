	journal = {Biochimica et Biophysica Acta (BBA) - Molecular Cell Research},
	author = {Zabrocki, Piotr and Bastiaens, Ilse and Delay, Charlotte and Bammens, Tine and Ghillebert, Ruben and Pellens, Klaartje and De Virgilio, Claudio and Van Leuven, Fred and Winderickx, Joris},
	month = oct,
