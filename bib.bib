	pmid = {10391881},
	pages = {19509--19512},
	file = {Full Text PDF:/Users/pochtalionizm/.zotero/storage/VAZE8U9M/Wood и др. - 1999 - α-Synuclein Fibrillogenesis Is Nucleation-dependen.pdf:application/pdf;Snapshot:/Users/pochtalionizm/.zotero/storage/ZD2SUIVU/19509.html:text/html}
}

@article{uversky_protein-chameleon:_2003,
